  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build lib/main.js",
    "format": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nils-braun/pytest-benchmark-commenter"
  },
